<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationAttributeSet">
    <xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]</xpath>
    <attribute>ParentName</attribute>
    <value>PlantBaseNonEdible</value>
  </Operation>
  <Operation Class="PatchOperationInsert">
    <xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]/minifiedDef</xpath>
    <value>
      <selectable>true</selectable>
    </value>
  </Operation>
  <Operation Class="PatchOperationInsert">
    <xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]/plant/harvestYield</xpath>
    <value>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
    </value>
  </Operation>
  <Operation Class="PatchOperationInsert">
    <xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]/plant/harvestYield</xpath>
    <value>
      <harvestedThingDef>WoodLog</harvestedThingDef>
    </value>
  </Operation>
  <Operation Class="PatchOperationInsert">
    <xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]/plant/harvestYield</xpath>
    <value>
      <harvestTag>Wood</harvestTag>
    </value>
  </Operation>
</Patch>
